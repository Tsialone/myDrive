CREATE TABLE users (
    idUser VARCHAR(100)  PRIMARY KEY   NOT NULL ,
    email VARCHAR(100)  NOT NULL ,
    name VARCHAR(100) NOT NULL  ,
    password VARCHAR(100)  NOT NULL
);
CREATE TABLE solde (
    idSolde VARCHAR(100)   PRIMARY KEY NOT NULL  , 
    montant FLOAT NOT NULL ,
    date DATE NOT NULL
);
CREATE TABLE users_solde (
    idSolde VARCHAR(100) , 
    idUser VARCHAR(100) , 
    PRIMARY KEY (idSolde , idUser)
);
